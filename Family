import java.util.Scanner;

public class Family {

    public static void main(String[] args) {
        int option = -1;
        Scanner scanchoice = new Scanner(System.in);
        while (option < 1 || option > 7) {
            System.out.println("1- Boss:");
            System.out.println("2- UnderBoss:");
            System.out.println("3- Consigliere:");
            System.out.println("4- Soldier:");
            System.out.println("5- Associates:");
            System.out.println("6- CapoRegime:");
            System.out.println("Escolha a opção:");
            if (scanchoice.hasNextInt()) {
                option = scanchoice.nextInt();
            }
        }
        switch (option) {
            case 1:
                System.out.println("Boss");
                break;
            case 2:
                System.out.println("UnderBoss");
                break;
            case 3:
                System.out.println("Consigliere");
                break;
            case 4:
                System.out.println("Soldier");
                break;
            case 5:
                System.out.println("Associates");
                break;
            case 6:
                System.out.println("CapoRegime");
                break;
            default:
                System.out.println("Opcao invalida");
        }
    }

}
